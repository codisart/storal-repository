{
    "name": "codisart/storal-repository",
    "description": "A proposal on how to use laminas/laminas-db",
    "type": "library",
    "require": {
        "php": "^8.0",
        "laminas/laminas-db": "^2.11",
        "laminas/laminas-hydrator": "^3.0 || ^4.0"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "mockery/mockery": "^1.3",
        "phpro/grumphp": "^1.2.0",
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.16 || ^3.0",
        "rector/rector": "^0.15.1 || ^0.17.0"
    },
    "autoload": {
        "psr-4": {"Storal\\": "src/"}
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
